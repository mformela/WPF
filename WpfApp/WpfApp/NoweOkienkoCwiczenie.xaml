<Window x:Class="WpfApp.NoweOkienkoCwiczenie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp"
        mc:Ignorable="d"
        Title="NoweOkienkoCwiczenie" Height="300" Width="300">

    <DockPanel>
        <StackPanel x:Name="CheckBoxStack">
            <CheckBox IsChecked="{Binding Path=TestChecked}"
                x:Name="bloczek" Content="test"></CheckBox>
            <Button x:Name="setTrue" Content="set true" Padding="10" Click="setTrue_Click"/>
            <Button x:Name="setFalse" Content="set true" Padding="10" Click="setFalse_Click"/>
           
        </StackPanel>



    </DockPanel>

</Window>
